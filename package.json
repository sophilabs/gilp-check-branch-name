  },
  "homepage": "https://github.com/sophilabs/gilp-check-branch-name",
  "dependencies": {
    "gilp-util": "^0.0.6",
    "gulp-util": "^3.0.7",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "gilp-util": "^1.1.1",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "rewire": "^2.5.2",
    "semistandard": "^8.0.0",
    "should": "^9.0.2",
    "vinyl": "^1.1.1"
